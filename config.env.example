# Variables de entorno para el bot de HermessApp
# Copia este archivo como config.env y completa con tus credenciales

# Credenciales de HermessApp (requeridas)
HERMESS_EMAIL=
HERMESS_PASSWORD=
HERMESS_LOGIN_URL=https://hermessapp.com/login
HERMESS_BIRTHDAYS_URL=https://hermessapp.com/pacientescumple

# Configuraci√≥n del entorno (opcionales)
# Valores: shared_hosting, vps
ENVIRONMENT=shared_hosting

# Modo headless del navegador (opcional)
# Valores: true, false
HEADLESS=true

# Puerto del servidor (opcional)
# Por defecto: 5000
PORT=5000

# Modo debug (opcional)
# Valores: true, false
DEBUG=false